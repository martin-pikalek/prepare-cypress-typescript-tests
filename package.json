{
  "name": "prepare-cypress-typescript-tests",
  "version": "1.0.0",
  "description": "Cypress typescript template",
  "scripts": {
    "cy:e2e:open": "npx cypress open --e2e",
    "cy:e2e:run": "npx cypress run --e2e",
    "commit": "commit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martin-pikalek/prepare-cypress-typescript-tests.git"
  },
  "keywords": [
    "cypress",
    "typescript",
    "template"
  ],
  "author": "Martin Pik√°lek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martin-pikalek/prepare-cypress-typescript-tests/issues"
  },
  "homepage": "https://github.com/martin-pikalek/prepare-cypress-typescript-tests#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "cypress": "^10.1.0",
    "dayjs": "^1.11.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/prompt-cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  }
}
